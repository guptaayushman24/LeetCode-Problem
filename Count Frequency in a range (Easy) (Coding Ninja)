import java.util.*;
public class Solution {
    public static int[] countFrequency(int n, int x, int []nums){
         int hash_arr[]=new int[x+1];

        for(int i=0;i<n;i++)

        {

            hash_arr[nums[i]]=hash_arr[nums[i]]+1;

        }

        return Arrays.copyOfRange(hash_arr, 1, n+1);
    }
}

class Solution {
    public int minOperations(int[] nums, int x) {
       int sum = 0;
    for (int i=0;i<nums.length;i++){
        sum = sum+nums[i];
    }

    int maxLength = -1;
    int currSum = 0;
    int l = 0;
    int r = 0;
    
    while (r < nums.length) {
        currSum += nums[r];
        while (l <= r && currSum > sum - x) {
            currSum -= nums[l];
            l++;
        }
        
        if (currSum == sum - x) {
            maxLength = Math.max(maxLength, r - l + 1);
        }
        
        r++;
    }

    if (maxLength == -1) {
        return -1;
    } else {
        return nums.length - maxLength;
 
    }
    }
}

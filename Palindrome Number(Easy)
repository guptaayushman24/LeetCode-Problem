class Solution {
    public boolean isPalindrome(int x) {
        
        String str = Integer.toString(x);
        String rev_str="";
        for (int i=str.length()-1;i>=0;i--){
            rev_str+=str.charAt(i);
        }

        if (str.equals(rev_str)){
            return true;
        }
        else{
            return false;
        }
    }

}
